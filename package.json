{
  "name": "@wppconnect/libsignal-protocol",
  "version": "0.0.16",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/wppconnect-team/libsignal-protocol",
  "author": "Rolfe Schmidt <rolfeschmidt@gmail.com>",
  "license": "GPL-3.0-only",
  "scripts": {
    "test": "jest --config jest.config.js",
    "lint": "eslint -c .eslintrc.js  '**/*.ts'",
    "format": "prettier '**/{*.{js?(on),ts?(x),md},.*.js?(on)}' --write --list-different --config prettier.config.js",
    "prepare": "npm run build",
    "build": "tsc -d",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint && npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "compile-protos:linux": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/protos/ --ts_proto_opt=env=both --ts_proto_opt=exportCommonSymbols=false -I ./protos ./protos/WhisperTextProtocol.proto",
    "compile-protos:windows": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_out=./src/protos/ --ts_proto_opt=env=both --ts_proto_opt=exportCommonSymbols=false -I ./protos ./protos/WhisperTextProtocol.proto"
  },
  "devDependencies": {
    "@protobuf-ts/protoc": "^2.10.0",
    "@types/base64-js": "^1.3.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^22.15.20",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.3",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-proto": "^2.7.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.4.0",
    "@privacyresearch/curve25519-typescript": "^0.0.12"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/internal/**/*"
  ]
}
